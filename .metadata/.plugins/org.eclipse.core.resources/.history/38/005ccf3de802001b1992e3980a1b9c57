package org.jspider.manyApp;
import java.util.List;

import javax.persistence.*;
@Entity
@Table(name="Person_Detals")
public class Person 
{
	private int rollno;
	private String name;
	private List<BankAccount> acc;
	
	public int getRollno() {
		return rollno;
	}
	public void setRollno(int rollno) {
		this.rollno = rollno;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public java.util.List<BankAccount> getAcc() {
		return acc;
	}
	public void setAcc(java.util.List<BankAccount> acc) {
		this.acc = acc;
	}
	
}
