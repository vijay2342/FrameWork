package org.jspider.manyApp;
import javax.persistence.Entity;
import javax.persistence.Table;

import antlr.collections.List;
@Entity
@Table(name="Person_Detals")
public class Person 
{
	private int rollno;
	private String name;
	private java.util.List<BankAccount> acc;
	
	public int getRollno() {
		return rollno;
	}
	public void setRollno(int rollno) {
		this.rollno = rollno;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public java.util.List<BankAccount> getAcc() {
		return acc;
	}
	public void setAcc(java.util.List<BankAccount> acc) {
		this.acc = acc;
	}
	
}
